{
  "name": "synapse-feeder",
  "version": "1.0.0",
  "type": "module",
  "description": "Daily Crawling Automation System - 매일 돌아가지만 아무도 관리하지 않는 시스템",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "tsx src/main.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "install:skip-build": "npm install --ignore-scripts && npm rebuild better-sqlite3 --build-from-source=false"
  },
  "keywords": [
    "crawler",
    "automation",
    "data-collection",
    "batch"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@mozilla/readability": "^0.5.0",
    "@postlight/parser": "^2.2.3",
    "@sentry/node": "^8.45.0",
    "@slack/web-api": "^7.9.0",
    "axios": "^1.7.9",
    "axios-retry": "^4.4.0",
    "better-sqlite3": "^12.6.2",
    "bottleneck": "^2.19.5",
    "cheerio": "^1.0.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "handlebars": "^4.7.8",
    "iconv-lite": "^0.6.3",
    "jsdom": "^24.1.3",
    "node-cache": "^5.1.2",
    "normalize-url": "^7.0.1",
    "p-retry": "^6.2.0",
    "pino": "^9.5.0",
    "pino-pretty": "^12.0.0",
    "playwright": "^1.48.2",
    "rss-parser": "^3.13.0",
    "sanitize-html": "^2.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/cheerio": "^0.22.35",
    "@types/handlebars": "^4.1.0",
    "@types/iconv-lite": "^0.0.1",
    "@types/node": "^22.10.5",
    "@types/sanitize-html": "^2.13.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
